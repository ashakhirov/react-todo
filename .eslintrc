{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [],
  "rules": {
    "no-unused-vars": "warn",
    "arrow-body-style": "off",
    "no-shadow": "off",
    "arrow-parens": "off",
    "jsx-a11y/label-has-associated-control": "off",
    "jsx-a11y/label-has-for": "off"
  }
}
